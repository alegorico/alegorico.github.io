{
  "name": "@chrisdiana/cmsjs",
  "version": "2.0.1",
  "description": "The JavaScript Site Generator.",
  "keywords": "client-side, javascript, site-generator",
  "homepage": "https://github.com/chrisdiana/cms.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all2 --parallel serve watch",
    "serve": "serve . -p 3000",
    "start": "npm run dev",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "clean": "echo 'Clean: manually delete dist/cms.js and examples/js/cms.js'"
  },
  "main": "dist/cms.js",
  "module": "dist/cms.es.js",
  "browser": "dist/cms.min.js",
  "unpkg": "dist/cms.js",
  "jsdelivr": "dist/cms.js",
  "files": [
    "src",
    "dist/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/chrisdiana/cms.js.git"
  },
  "author": {
    "name": "Chris Diana"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisdiana/cms.js/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "acorn": ">=8.12.0",
    "braces": ">=3.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "ini": ">=4.1.3",
    "js-yaml": ">=4.1.0",
    "lodash": ">=4.17.21",
    "minimist": ">=1.2.8",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "rollup": "^4.22.0",
    "rollup-plugin-livereload": "^2.0.5",
    "serve": ">=14.2.3",
    "typescript": "^5.6.2"
  }
}
